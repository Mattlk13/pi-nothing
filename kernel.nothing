;; -*- mode: scheme -*-

(define (putc ch)
  (! #x101f1000 ;; UART0
     ch))

(define (puts p)
  (let ((ch (?byte p)))
    (if ch
	(begin (putc ch)
	       (puts (+ p 1)))
	0)))

(define (main)
  (puts (data #"Hello, world!\n\0"))
